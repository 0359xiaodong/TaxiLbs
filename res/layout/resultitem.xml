        android:layout_height="wrap_content"
        android:layout_marginLeft="6.0dip"
        android:orientation="vertical"
        android:paddingRight="4.0dip" >

        <FrameLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/name"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:ellipsize="marquee"
                android:singleLine="true"
                android:textAppearance="?android:textAppearanceMedium"
                android:textColor="#ff000000" />
            <!--
            <ImageView
                android:id="@+id/tuan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="right|center"
                android:padding="2.0dip"
                android:src="@drawable/ic_tuan_small" />

            <ImageView
                android:id="@+id/promo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="right|center"
                android:padding="2.0dip"
                android:src="@drawable/ic_promo_small" />

            <ImageView
                android:id="@+id/checkin"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="right|center"
                android:padding="2.0dip"
                android:src="@drawable/ic_checkin_small" />

            <ImageView
                android:id="@+id/card"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="right|center"
                android:padding="2.0dip"
                android:src="@drawable/ic_card_small" />
            -->
        </FrameLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="3.0dip"
            android:orientation="horizontal" >

            <!--
            <com.example.dazhong1.PoiStar
                android:id="@+id/poistar"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="left|center" />
            -->

            <TextView
                android:id="@+id/start_str"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="4.0dip"
                android:singleLine="true"
                android:textAppearance="?android:textAppearanceSmall"
                android:textColor="#ff000000"
                android:textSize="14.0sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="3.0dip"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/end_str"
                android:layout_width="0.0px"
                android:layout_height="wrap_content"
                android:layout_weight="1.0"
                android:singleLine="true"
                android:textAppearance="?android:textAppearanceSmall"
                android:textColor="#ff000000"
                android:textSize="14.0sp" />

            <TextView
                android:id="@+id/distance"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:singleLine="true"
                android:textAppearance="?android:textAppearanceSmall"
                android:textColor="#ff000000"
                android:textSize="14.0sp" />
        </LinearLayout>
    </LinearLayout>

</com.taxiCliect.activity.person.OftenRouteListItem>